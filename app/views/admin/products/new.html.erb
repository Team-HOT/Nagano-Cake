<h1>商品　新規登録画面</h1>
<h1>新規投稿</h1>
<!-- form_for部分 -->
<%= form_for(@product, url: admin_products_index_path, method: :get) do |f| %>
 
    <h4>タイトル</h4>
    <%= f.text_field :name %>
 
    <h4>本文</h4>
    <%= f.text_area :price %>
    <h4>画像</h4>
  <%= f.attachment_field :product_image %>
 
    <%= f.submit '投稿' %>
 
<% end %>



